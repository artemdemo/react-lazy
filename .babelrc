{
    "presets": [
        "react",
        ["env", {
            "targets": {
                "browsers": ["last 2 versions", "safari >= 7"]
            }
        }]
    ],
    "plugins": [
        "transform-react-jsx",
        "transform-object-assign",
        "syntax-dynamic-import",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ]
}
