{
  "name": "react-webpack-starter",
  "version": "1.0.0",
  "description": "Experiments with lazy components and routes in React",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run server:dev",
    "server:dev": "webpack-dev-server",
    "build": "webpack --progress --colors",
    "npm-check": "npm outdated",
    "test": "BABEL_DISABLE_CACHE=1 jest --config ./jest-config.json",
    "eslint": "./node_modules/eslint/bin/eslint.js ./source/**/*.js ./source/**/**/*.js ./source/**/*.jsx ./source/**/**/*.jsx",
    "watch": "webpack --progress --colors --watch",
    "stats": "webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json -p 8888 -r bundle-report.html",
    "postinstall": "./postinstall.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemdemo/react-webpack-starter.git"
  },
  "author": "Artem Demo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artemdemo/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/artemdemo/react-webpack-starter#readme",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^3.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "reselect": "^3.0.1",
    "style-loader": "^0.19.0",
    "superagent": "^3.8.0",
    "superagent-bluebird-promise": "^4.2.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/prop-types": "^15.5.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.22.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "enzyme": "^3.1.0",
    "eslint-config-artem-react": "git+https://github.com/artemdemo/eslint-config-artem-react.git#3.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.1",
    "jest-cli": "^21.2.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-chunk-hash": "^0.5.0",
    "webpack-dev-server": "^2.9.3"
